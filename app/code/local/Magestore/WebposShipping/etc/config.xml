<config>
    <modules>
        <Magestore_WebposShipping>
            <version>1.0.0</version>
        </Magestore_WebposShipping>
    </modules>
    <frontend>
        <routers>
            <webposshipping>
                <use>standard</use>
                <args>
                    <module>Magestore_WebposShipping</module>
                    <frontName>webposshipping</frontName>
                </args>
            </webposshipping>
        </routers>
        <layout>
            <updates>
                <webposshipping>
                    <file>webposshipping.xml</file>
                </webposshipping>
            </updates>
        </layout>
    </frontend>
    <global>
        <models>
            <webposshipping>
                <class>Magestore_WebposShipping_Model</class>
                <resourceModel>Magestore_WebposShipping_mysql4</resourceModel>
            </webposshipping>
            <webposshipping_mysql4>
                <class>Magestore_WebposShipping_Model_Mysql4</class>
                <entities>
                </entities>
            </webposshipping_mysql4>
            <webpos>
                <rewrite>
                    <source_adminhtml_shipping>Magestore_WebposShipping_Model_Rewrites_Webpos_Source_Adminhtml_Shipping</source_adminhtml_shipping>
                    <source_adminhtml_shippingoffline>Magestore_WebposShipping_Model_Rewrites_Webpos_Source_Adminhtml_Shippingoffline</source_adminhtml_shippingoffline>
                    <checkout_create>Magestore_WebposShipping_Model_Rewrites_Webpos_Checkout_Create</checkout_create>
                </rewrite>
            </webpos>
            <!--<mpshippingcanadapostal>-->
                <!--<rewrite>-->
                    <!--<carrier_localdelivery>Magestore_WebposShipping_Model_Rewrites_Mpshippingcanadapostal_Carrier_LocalDelivery</carrier_localdelivery>-->
                <!--</rewrite>-->
            <!--</mpshippingcanadapostal>-->
        </models>
        <resources>
            <webposshipping_setup>
                <setup>
                    <module>Magestore_WebposShipping</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </webposshipping_setup>
            <webposshipping_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </webposshipping_write>
            <webposshipping_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </webposshipping_read>
        </resources>
        <blocks>
            <webposshipping>
                <class>Magestore_WebposShipping_Block</class>
            </webposshipping>
        </blocks>
        <helpers>
            <webposshipping>
                <class>Magestore_WebposShipping_Helper</class>
            </webposshipping>
        </helpers>
        <events>
            
        </events>
    </global>
    <default>
    </default>
</config>

