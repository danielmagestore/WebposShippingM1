<layout version="0.1.0">
    <webpos_index_index>
        <reference name="before_webpos_body_end">
            <block type="webposshipping/webposShipping" name="webposshipping" as="webposshipping" template="webposshipping/webposShipping.phtml"/>
        </reference>
        <reference name="webpos_content_main">
            <action method="addJsLayout">
                <arg>
                    <components>
                        <webpos.container>
                            <children>
                                <checkout>
                                    <children>
                                        <checkout>
                                            <children>
                                                <webpos_checkout_shipping>
                                                    <component>ui/components/webposshipping/checkout/checkout/shipping</component>
                                                </webpos_checkout_shipping>
                                            </children>
                                        </checkout>
                                    </children>
                                </checkout>
                            </children>
                        </webpos.container>
                    </components>
                </arg>
            </action>
        </reference>
    </webpos_index_index>
</layout>
